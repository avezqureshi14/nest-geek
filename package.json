{
  "name": "auth-module",
  "version": "0.0.1",
  "prisma": {
    "seed": "ts-node src/db/prisma/seed/roles_permission.seed.ts"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate:enum": "npx ts-node src/common/enums/generator/enum.generator.ts",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:dev:deploy": "npx prisma migrate deploy --schema=./src/db/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./src/db/prisma/schema.prisma --create-only",
    "prisma:dbpull": "npx prisma db pull --schema=./src/db/prisma/schema.prisma ",
    "prisma:seed": "npx prisma db seed",
    "db:dev:rm": "docker compose rm -f -s -v",
    "db:dev:up": "docker compose up -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
    "start:migrate:prod": "npx prisma migrate deploy --schema=./src/db/prisma/schema.prisma && npm run start:prod",
    "prisma:studio": "npx dotenv -e ./.env.local -- npx prisma studio --schema=./src/db/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/db/prisma/schema.prisma",
    "prisma:setup": "npm run prisma:dev:deploy && npm run prisma:dbpull && npm run prisma:generate"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@aws-sdk/client-sns": "^3.616.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.17.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "firebase-admin": "^11.11.0",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.2.0",
    "graphql": "^16.8.1",
    "kafkajs": "^2.2.4",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "twilio": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.20",
    "@types/hapi__joi": "^17.1.12",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-facebook": "^3.0.2",
    "@types/passport-google-oauth20": "^2.0.13",
    "@types/passport-jwt": "^3.0.12",
    "@types/passport-linkedin-oauth2": "^1.5.5",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.17.0",
    "prisma-nestjs-graphql": "^20.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
